<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>Opciones</servlet-name>
        <servlet-class>Opciones</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Query</servlet-name>
        <servlet-class>Query</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Query2</servlet-name>
        <servlet-class>Query2</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Abm</servlet-name>
        <servlet-class>Abm</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Query3</servlet-name>
        <servlet-class>Query3</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Query4</servlet-name>
        <servlet-class>Query4</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>Opciones</servlet-name>
        <url-pattern>/Opciones</url-pattern>
    </servlet-mapping>
     <servlet-mapping>
        <servlet-name>Query</servlet-name>
        <url-pattern>/Query</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Query2</servlet-name>
        <url-pattern>/Query2</url-pattern>
    </servlet-mapping>  
     <servlet-mapping>
        <servlet-name>Query3</servlet-name>
        <url-pattern>/Query3</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Query4</servlet-name>
        <url-pattern>/Query4</url-pattern>
    </servlet-mapping>  
    <servlet-mapping>
        <servlet-name>Abm</servlet-name>
        <url-pattern>/Abm</url-pattern>
    </servlet-mapping>  
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
    <welcome-file-list>
        <welcome-file>index.html</welcome-file>
    </welcome-file-list>
</web-app>
